               ‚àô cong ((‚Ñï.remainder 2 ‚Ñï.+ n / 2) ‚Ñï.+_)
             ((cong ((‚Ñï.quotient 2 ‚Ñï.+ n / 2) ‚Ñï.+_)
              (sym (‚Ñï.+-zero (‚Ñï.quotient 2 ‚Ñï.+ n / 2)))))
             ‚àô z)
           (cong ((2 ^ k) ‚Ñï.+_) (sym (‚Ñï.+-zero (2 ^ k))))
