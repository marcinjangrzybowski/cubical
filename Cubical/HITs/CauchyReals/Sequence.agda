   let (k , (X , Lst)) = w N
          (‚Ñï.quotient 2 ‚Ñï.+ n / 2)
          (‚Ñï.‚â§-trans (‚Ñï.pred-‚â§-pred (‚Ñï.2‚â§x‚Üíquotient[x/2]<x n))
