+ùï° : ‚àÄ n {m} ‚Üí ‚Ñôrm‚Çê {true} m ‚Üí ‚Ñôrm‚Çê {true} (n + m) 
+ùï° n = ‚ÑôrmRecElimN.f w
 where
 open ‚ÑôrmRecElimN
 w : ‚ÑôrmRecElimN (Œª m ‚Üí ‚Ñôrm‚Çê (n + m))
 abase w _ = ùï°base
 aloop w m x = ùï°loop (m+AB n x)
 ainvol w _ _ _ ig =
   ùï°invol _ _ (record { ab= =
     AB‚â° _ _ _
   refl
   refl
   refl
   refl
     ‚àô cong (m+AB n) (ab= ig) })
   where
    open involGuard
 ahex w m _ _ _ ig =
   ùï°hex _ _ _ ww
   where
    open AB
    open hexGuard
    ww : hexGuard _ _ _
    =lPad ww = cong (n ‚Çê‚äπ_) (ig .=lPad)
    =rPad ww = ig .=rPad
    l= ww = ig .l=
    l=' ww = ig .l='
    c+r ww = ig .c+r
    
 acomm w m _ _ g =
  ùï°comm _ _ ww
   where
    open AB
    open commGuard
    ww : commGuard _ _
    cPad ww = cPad g
    lpad= ww = cong (n ‚Çê‚äπ_) (lpad= g) ‚àô
       cong just +-+‚Çê‚â°‚Çê+-+
    
 aover w m _ _ _ g =
  ùï°over _ _ _ ww
   where
    open AB
    open overGuard
    ww : overGuard _ _ _
    =lPad ww = cong (n ‚Çê‚äπ_) (=lPad g)
    =rPad ww = =rPad g
    r=l ww = r=l g
    l=r ww = l=r g
    l+r ww = l+r g
    
 asquash w _ = ùï°squash _


ùï°+ : ‚àÄ n {m} ‚Üí ‚Ñôrm‚Çê {true} m ‚Üí ‚Ñôrm‚Çê {true} (m + n) 
