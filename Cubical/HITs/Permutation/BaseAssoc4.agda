    ww : hexGuard _ _ _
    =lPad ww = (ig .=lPad)
    =rPad ww = cong (_‚äπ‚Çê n) (ig .=rPad)
    l= ww = ig .l=
    l=' ww = ig .l='
    c+r ww = ig .c+r
    
 acomm w m _ _ g =
  ùï°comm _ _ ww
   where
    open AB
    open commGuard
    ww : commGuard _ _
    cPad ww = cPad g
    lpad= ww = lpad= g
    
 aover w m _ _ _ g =
  ùï°over _ _ _ ww
   where
    open AB
